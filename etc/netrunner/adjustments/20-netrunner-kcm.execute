#!/usr/bin/env python
import os
source_dir = "/usr/share/netrunner-default-settings/kde4-profile/default/share/kde4/services"
target_dir = "/usr/share/kde4/services"
for file in os.listdir(source_dir):
    if os.path.isfile(os.path.join(source_dir, file)) and file.endswith(".desktop"):
        file = os.path.join(target_dir, file)
        newfile = file[:-7]+'old'
        print newfile
        try:        
            os.rename(file, newfile)
        except:
            pass
